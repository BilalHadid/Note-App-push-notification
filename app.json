{
  "expo": {
    "name": "NoteApp",
    "slug": "noteapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LLogo2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.yourcompany.yourappname",
     "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.notecompany.noteapp",
      "useNextNotificationsApi": true,
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}

 